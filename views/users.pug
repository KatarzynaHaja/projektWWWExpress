extend layout.pug
block content
    div.container
        form.login-form.form-horizontal(method='post', role='form')
            div.col-sm-8.col-sm-offset-4
                div.form-group
                    div.row.align-item-start

                        each val in form
                            li
                                label= val.username
                                    input(type="checkbox" name="user" value=val.username)

            block button


